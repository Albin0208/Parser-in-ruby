func int fib(int value){
    if value <= 0{
        return 0
    }
    elsif value == 1 || value == 2{
        return 1
    }
    else{
        return fib(value-1) + fib(value-2)
    }
}
print(fib(0))     #0
print(fib(4))     #2
print(fib(27))    #6765

